--diana warrior

function _init()
	create_player()
	h = create_harceleur()
 --create_enemies()
	init_msg()	
	
	end
	


function _update()
if not msg_list[1] then
 player_movement()
 end
 update_camera()
 update_msg()
 
end

function _draw()
cls()
 draw_map()
	draw_player()
--	draw_enemies()
	draw_ui()
	draw_msg()
	
end
